    "s = '(((((((())))()'\n",
    "\n",
    "def solution(s):\n",
    "    wt = 0\n",
    "    for c in s :\n",
    "        if c == '(' : wt += 1\n",
    "        elif c == ')' : wt -= 1\n",
    "        if wt < 0 : return False\n",
    "    return wt == 0"
   ]
